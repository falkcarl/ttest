<span id="cb6-23"><a href="#cb6-23" tabindex="-1"></a><span class="co">#&gt; 8          2.6         1.2 versicolor</span></span>
<span id="cb6-24"><a href="#cb6-24" tabindex="-1"></a><span class="co">#&gt; 9          2.7         1.9  virginica</span></span>
<span id="cb6-25"><a href="#cb6-25" tabindex="-1"></a><span class="co">#&gt; 10         3.0         1.4 versicolor</span></span>
<span id="cb6-26"><a href="#cb6-26" tabindex="-1"></a></span>
<span id="cb6-27"><a href="#cb6-27" tabindex="-1"></a><span class="fu">data_select</span>(iris, <span class="fu">contains</span>(<span class="st">&quot;pal&quot;</span>, <span class="st">&quot;ec&quot;</span>))</span>
