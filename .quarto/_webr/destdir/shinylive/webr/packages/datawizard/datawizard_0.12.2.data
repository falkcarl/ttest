<span id="cb6-12"><a href="#cb6-12" tabindex="-1"></a><span class="co">#&gt; 10          6.1         3.0          4.6         1.4</span></span>
<span id="cb6-13"><a href="#cb6-13" tabindex="-1"></a></span>
<span id="cb6-14"><a href="#cb6-14" tabindex="-1"></a><span class="fu">data_select</span>(iris, <span class="fu">ends_with</span>(<span class="st">&quot;dth&quot;</span>, <span class="st">&quot;ies&quot;</span>))</span>
<span id="cb6-15"><a href="#cb6-15" tabindex="-1"></a><span class="co">#&gt;    Sepal.Width Petal.Width    Species</span></span>
<span id="cb6-16"><a href="#cb6-16" tabindex="-1"></a><span class="co">#&gt; 1          3.0         0.1     setosa</span></span>
<span id="cb6-17"><a href="#cb6-17" tabindex="-1"></a><span class="co">#&gt; 2          3.3         0.2     setosa</span></span>
<span id="cb6-18"><a href="#cb6-18" tabindex="-1"></a><span class="co">#&gt; 3          3.8         2.2  virginica</span></span>
<span id="cb6-19"><a href="#cb6-19" tabindex="-1"></a><span class="co">#&gt; 4          3.2         0.2     setosa</span></span>
