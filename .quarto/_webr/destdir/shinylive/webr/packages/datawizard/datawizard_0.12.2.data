<span id="cb12-7"><a href="#cb12-7" tabindex="-1"></a>      <span class="at">DF =</span> df_name,</span>
<span id="cb12-8"><a href="#cb12-8" tabindex="-1"></a>      <span class="at">Mean =</span> <span class="fu">mean</span>(Subjective_Valence),</span>
<span id="cb12-9"><a href="#cb12-9" tabindex="-1"></a>      <span class="at">SD =</span> <span class="fu">sd</span>(Subjective_Valence)</span>
<span id="cb12-10"><a href="#cb12-10" tabindex="-1"></a>    ) <span class="sc">%&gt;%</span></span>
<span id="cb12-11"><a href="#cb12-11" tabindex="-1"></a>    <span class="fu">head</span>(<span class="dv">5</span>) <span class="sc">%&gt;%</span></span>
<span id="cb12-12"><a href="#cb12-12" tabindex="-1"></a>    <span class="fu">select</span>(DF, <span class="fu">everything</span>())</span>
<span id="cb12-13"><a href="#cb12-13" tabindex="-1"></a>}</span>
<span id="cb12-14"><a href="#cb12-14" tabindex="-1"></a></span>
<span id="cb12-15"><a href="#cb12-15" tabindex="-1"></a><span class="co"># Check the results</span></span>
